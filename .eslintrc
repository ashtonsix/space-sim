{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "rules": {
    // THIS IS SPARTA!!
    "id-length": 0,
    // THIS IS... expressions
    "no-nested-ternary": 0,
    "no-sequences": 0,
    // console <3
    "no-console": 0,
    // nah mate, no semi
    "semi": [2, "never"],
    "no-unexpected-multiline": 2,
    // like it like that
    "object-curly-spacing": [2, "never"],
    // some things are private
    "no-underscore-dangle": 0,
    // big screen
    "max-len": [2, 130],
  },
  "globals": {
    "_": true,
    "THREE": true
  }
}
